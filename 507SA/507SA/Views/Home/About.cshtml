
@{
    ViewBag.Title = "About";
}

<div class="=row">
    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <table class="table">
            <thead>
                <tr>
                    <th><p>Фамілія</p></th>
                    <th><p>Ім'я</p></th>
                    <th><p>Телефон</p></th>
                    <th><p>Інформація</p></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var b in ViewBag.Users)
                {
                    <tr>
                        <td>@b.LastName</td>
                        <td>@b.FirstName</td>
                        <td>@b.Phone</td>
                        <td>@b.Info</td>
                    </tr>}
            </tbody>

        </table>
    </div>
</div>

